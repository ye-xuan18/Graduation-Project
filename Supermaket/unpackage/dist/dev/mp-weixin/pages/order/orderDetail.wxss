}
.goods-item-view .goods-info .goods-title {
  margin-bottom: 10rpx;
  color: #303133;
}
.goods-item-view .goods-info .goods-specs {
  font-size: 24rpx;
  margin-bottom: 10rpx;
  color: #cccccc;
}
.goods-item-view .goods-info .goods-price {
